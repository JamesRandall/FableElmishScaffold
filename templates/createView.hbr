module {{{rootNamespace}}}.Create.View
open Elmish
open Elmish.React
open Fable.React
open Fable.React.Props
open Browser

open Types

let root context model dispatch =
    div [] [
      h1 [] [str "Create New {{{entityName}}}"]
      p [] [a [Router.href (Router.{{{entityName}}} <| Router.{{{entityName}}}Page.Index)] [str "Back"]]
      table [] [
        tbody [] [
          tr [] [
            th [] [str "Value"]
            td [] [input [Value model.AProperty ; OnChange (fun ev -> ev.Value |> ChangeAProperty |> dispatch)]]
          ]
        ]
      ]
      button [OnClick (fun _ -> console.log("Submitting with property value " + model.AProperty) ; Submit |> dispatch)] [str "Submit"]
    ]
